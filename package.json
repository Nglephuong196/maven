    "create-invalidation:dev": "aws cloudfront create-invalidation --distribution-id EW7HLYQ0H6KJ2 --paths /*",
    "build:staging": "./node_modules/.bin/env-cmd -f .env.staging next build && next export",
    "deployS3:staging": "aws s3 sync out s3://zien-maven-frontend-staging-2 --acl public-read",
    "create-invalidation:staging": "aws cloudfront create-invalidation --distribution-id E1NSAG5ZPABKGV --paths /*"
  },
